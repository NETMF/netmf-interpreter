    <NoWarn>$(NoWarn),0169</NoWarn>
    <ComponentGuid>{5a32a6b7-f7b8-4c87-bcce-b462f9a4217d}</ComponentGuid>
    <AssemblyBothEndian>true</AssemblyBothEndian>
  </PropertyGroup>
  <Import Project="$(SPOCLIENT)\tools\Targets\Microsoft.SPOT.CSharp.Targets" />
